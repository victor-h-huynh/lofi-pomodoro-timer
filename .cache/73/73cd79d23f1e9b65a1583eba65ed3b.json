ize: cover;\n  background-position: center;\n  height: 100vh;\n  position: relative;\n  backface-visibility: hidden;\n  animation: moveInLeft 2s ease-out; }\n\n.container {\n  position: absolute;\n  top: 60%;\n  left: 50%;\n  -moz-transform: translateX(-50%) translateY(-50%);\n  -webkit-transform: translateX(-50%) translateY(-50%);\n  transform: translateX(-50%) translateY(-50%);\n  text-shadow: 2px 2px #ff000050;\n  color: #ebebeb;\n  text-align: center;\n  width: 50%; }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Application */\n.header__text-box {\n  margin-bottom: 1rem; }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Buttons */\n.btn {\n  border: none;\n  outline: none;\n  padding: 1rem 1.6rem;\n  cursor: pointer;\n  transition-duration: 0.4s;\n  transition: all 0.2s; }\n  .btn--round {\n    border-radius: 10rem; }\n  .btn:hover {\n    transform: translateY(-0.2rem);\n    text-decoration: underline white 0.14rem; }\n  .btn:active {\n    transform: translateY(0rem); }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Sessions */\n.container__sessions {\n  font-size: 1rem;\n  margin-bottom: 1rem; }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Pomodoro Timer */\n/*////////////////////////////////////////////////////////////////////*/\n/*  Controls */\n/*////////////////////////////////////////////////////////////////////*/\n/*  Session Counter */\n/*////////////////////////////////////////////////////////////////////*/\n/*  States */\n.active {\n  text-decoration: underline white 0.14rem; }\n\n@keyframes moveInLeft {\n  0% {\n    opacity: 0;\n    transform: translateX(-3rem); }\n  100% {\n    opacity: 1;\n    transform: translateX(0); } }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  YouTube */\niframe {\n  transition: opacity 500ms ease-in-out;\n  transition-delay: 250ms; }\n\n/*//////////////