Sessions */\n.container__sessions {\n  font-size: 1rem;\n  margin-bottom: 1rem; }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Pomodoro Timer */\n/*////////////////////////////////////////////////////////////////////*/\n/*  Controls */\n/*////////////////////////////////////////////////////////////////////*/\n/*  Session Counter */\n/*////////////////////////////////////////////////////////////////////*/\n/*  States */\n.active {\n  text-decoration: underline white 0.14rem; }\n\n@keyframes moveInLeft {\n  0% {\n    opacity: 0;\n    transform: translateX(-3rem); }\n  100% {\n    opacity: 1;\n    transform: translateX(0); } }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  YouTube */\niframe {\n  transition: opacity 500ms ease-in-out;\n  transition-delay: 250ms; }\n\n/*////////////////////////////////////////////////////////////////////*/\n/*  Media Queries */\n@media all and (max-width: 810px) {\n  .container {\n    width: 90%; } }\n\n@media all and (max-width: 507px) {\n  .container {\n    width: 65%; } }\n\n@media all and (max-width: 443px) {\n  .container {\n    width: 74%; } }\n\n@media all and (max-width: 389px) {\n  .container {\n    width: 84%; } }\n"},"lineCount":null}},"error":null,"hash":"ea9177e48786e2a3dbf1bea7be14bef3","cacheData":{"env":{}}}